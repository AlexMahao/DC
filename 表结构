-- ----------------------------
-- Table structure for `t_comment`
-- ----------------------------
DROP TABLE IF EXISTS `t_comment`;
CREATE TABLE `t_comment` (
`id`  bigint(20) NOT NULL AUTO_INCREMENT COMMENT '主键' ,
`user_id`  bigint(20) NOT NULL COMMENT '用户id' ,
`content`  varchar(200) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '评论内容' ,
`channel`  char(2) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '渠道 （1:信息 2:心情）' ,
`channel_id`  bigint(20) NULL DEFAULT NULL COMMENT '渠道id' ,
`create_time`  datetime NULL DEFAULT NULL COMMENT '创建时间' ,
`praise_count`  int(20) NULL DEFAULT NULL COMMENT '点赞数' ,
`status`  char(2) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '状态（0:屏蔽 1:正常）' ,
PRIMARY KEY (`id`)
)
ENGINE=InnoDB
DEFAULT CHARACTER SET=utf8 COLLATE=utf8_general_ci
AUTO_INCREMENT=1

;



